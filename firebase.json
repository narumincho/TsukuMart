{
    "hosting": {
        "headers": {
            "source": "**",
            "headers": {
                "key": "cache-control",
                "value": "max-age=0"
            }
        },
        "public": "client/dist",
        "rewrites": [
            {
                "source": "sitemap",
                "function": "sitemap"
            },
            {
                "source": "**",
                "function": "indexHtml"
            }
        ]
    },
    "functions": {
        "source": "server/dist"
    }
}
