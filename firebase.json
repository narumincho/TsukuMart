{
    "hosting": {
        "public": "client/dist",
        "rewrites": [
            {
                "source": "/main.js",
                "destination": "/main.js"
            },
            {
                "source": "/style.css",
                "destination": "/style.css"
            },
            {
                "source": "/assets/**",
                "destination": "/assets/**"
            },
            {
                "source": "/redoc",
                "destination": "/redoc.html"
            },
            {
                "source": "/robots.txt",
                "destination": "/robots.txt"
            },
            {
                "source": "/api/sign-up",
                "function": "signUp"
            },
            {
                "source": "/api/token",
                "function": "getToken"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "functions": {
        "source": "server/dist"
    }
}
