{
    "hosting": {
        "public": "client/dist",
        "rewrites": [
            {
                "source": "/main.js",
                "destination": "/main.js"
            },
            {
                "source": "/style.css",
                "destination": "/style.css"
            },
            {
                "source": "/assets/**",
                "destination": "/assets/**"
            },
            {
                "source": "/robots.txt",
                "destination": "/robots.txt"
            },
            {
                "source": "/api",
                "function": "api"
            },
            {
                "source": "/social_login/google_receiver",
                "function": "googleLogInReceiver"
            },
            {
                "source": "/social_login/github_receiver",
                "function": "gitHubLogInReceiver"
            },
            {
                "source": "/social_login/twitter_receiver",
                "function": "twitterLogInReceiver"
            },
            {
                "source": "/social_login/line_receiver",
                "function": "lineLogInReceiver"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "functions": {
        "source": "server/dist"
    }
}
