{
    "hosting": {
        "headers": [
            {
                "source": "signup",
                "headers": [
                    {
                        "key": "content-type",
                        "value": "text/html"
                    },
                    {
                        "key": "cache-control",
                        "value": "max-age=0"
                    }
                ]
            },
            {
                "source": "**!(signup|signup.js|signup-call.js)",
                "headers": [
                    {
                        "key": "content-security-policy",
                        "value": "script-src https://tsukumart.com 'unsafe-inline'; worker-src https://tsukumart.com; img-src https://tsukumart.com https://asia-northeast1-tsukumart-f0971.cloudfunctions.net blob: data:; connect-src https://asia-northeast1-tsukumart-f0971.cloudfunctions.net/api; report-uri https://us-central1-tsukumart-f0971.cloudfunctions.net/contentSecurityPolicyReport"
                    },
                    {
                        "key": "cache-control",
                        "value": "max-age=0"
                    }
                ]
            }
        ],
        "public": "client/dist",
        "rewrites": [
            {
                "source": "sitemap",
                "function": "sitemap"
            },
            {
                "source": "**",
                "function": "indexHtml"
            }
        ]
    },
    "functions": {
        "source": "server/dist"
    }
}
